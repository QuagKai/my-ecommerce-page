<div class="mt-5">
    <div class="container mb-4">
        <div class="row row-cols-md-2 row-cols-1 bg-allvendor justify-content-center m-0 p-0" style="width: 100%">
            <div class="col d-flex justify-content-center align-items-center my-5" style="width: 25em">
                <a href="/vendor">
                    <img class="rounded-circle" src="images/AcneStudios_Logo.png" alt="acnestudio" style="width: 230px; height:230px;" >
                </a>
            </div>
                <div class="col p-0 m-0" style="width: 100%">
                    <div class="row d-flex p-0 m-0">
                        <% vendorProduct.forEach(function(product){ %>
                            <% if (product.creator == "Acne") { %>
                                <div class="card col p-0">
                                    <a href="/product/<%= product.id %>" class="text-black text-decoration-none">
                                        <div class="justify-content-center p-2" style="height: 20rem">
                                            <img class="card-img-top" src="images/<%= product.image.data.toString('ascii') %>" style="height: 100%; object-fit:cover" alt="<%= product.name %>">
                                        </div>
                                        <div class="card-body">
                                            <h4 class="card-title"><%= product.name %></h4>
                                            <p class="card-text"><%= product.price %> $</p>
                                            <a href="/add-to-cart/<%= product.id %> " class="btn btn-primary">Add to cart</a>
                                            <a href="#" class="btn btn-success">Buy now</a>
                                        </div>
                                    </a>
                                </div>
                            <% } %>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </div>
